{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 2,
    "name": "gallery-widget/gallery",
    "version": "1.0.0",
    "title": "MediaHUB Gallerie",
    "category": "media",
    "icon": "format-gallery",
    "description": "Zeigt eine Bildergalerie mit Dates oder Collections an",
    "supports": {
        "html": false,
        "align": [
            "wide",
            "full"
        ]
    },
    "attributes": {
        "selectedDates": {
            "type": "array",
            "default": []
        },
        "selectedCollections": {
            "type": "array",
            "default": []
        },
        "columns": {
            "type": "number",
            "default": 3
        },
        "showTitle": {
            "type": "boolean",
            "default": true
        }
    },
    "textdomain": "gallery-widget",
    "editorScript": "file:./index.js",
    "editorStyle": "file:./index.css",
    "style": "file:./style-index.css"
}
